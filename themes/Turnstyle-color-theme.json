{
	"name": "Turnstyle",
	"colors": {
    "sideBar.border": "#123734",
    "sideBarStickyScroll.border": "#123734",
    "focusBorder": "#123734",
    "tab.border": "#123734",
    "tab.activeBorder": "#123734",
    "tab.activeBorderTop": "#123734",
    "menu.border": "#123734",

    "sideBarTitle.background":"#232e4b",
    "tree.inactiveIndentGuidesStroke": "#8a5737",
    "tree.indentGuidesStroke": "#bf9346",
    "tree.tableColumnsBorder": "#bf9346",

    "tab.inactiveBackground": "#232e4b",
    "tab.unfocusedActiveBackground": "#232e4b",
    "titleBar.inactiveBackground": "#232e4b",

    "editorGroupHeader.tabsBackground": "#232e4b",

    "tab.activeBackground":"#0e283f",

    "sideBar.background": "#232e4b",
    "sideBar.foreground": "#c7cbd6",
    "sideBarSectionHeader.background": "#0e283f",
    "sideBarSectionHeader.foreground": "#c7cbd6",

		"editor.background": "#0e283f",
		"editor.foreground": "#c7cbd6",

    "editor.lineHighlightBorder": "#232e4b",

    "editor.selectionForeground": "#a7e5f0",
    "editor.selectionBackground": "#0b4bbd",
    "editor.selectionHighlightBackground": "#0b4bbd",
    "editor.selectionHighlightBorder": "#a7e5f0",
    "editor.wordHighlightBackground": "#0b4bbd",
    "editor.wordHighlightBorder": "#a7e5f0",
    "editor.wordHighlightStrongBackground": "#0b4bbd",
    "editor.wordHighlightStrongBorder": "#a7e5f0",

		"sideBarTitle.foreground": "#c7cbd6",
		"editorBracketHighlight.foreground1": "#5c93b8",
		"editorBracketHighlight.foreground2": "#5c93b8",
		"editorBracketHighlight.foreground3": "#5c93b8",
		"editorBracketHighlight.foreground4": "#5c93b8",
		"editorBracketHighlight.foreground5": "#5c93b8",
		"editorBracketHighlight.foreground6": "#5c93b8",
		"editorUnnecessaryCode.border": "#F35863ff",
		"activityBarBadge.background": "#be6d8d",
	},
	"tokenColors": [
    {
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#8a5737"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
    {
      "name": "Keyword, storages",
      "scope": [
        "keyword",
        "keyword.operator.pattern.case.shell",
        "keyword.operator.pattern.case.shell meta.case.entry.pattern.shell meta.case.shell source.shell",
        "storage",
        "keyword.other.package",
        "keyword.other.import",
        "keyword.other.package.java",
        "keyword.other.import.java"
      ],
      "settings": {
        "foreground": "#4391d4"
      }
    },
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#4391d4"
			}
		},
		{
			"name": "Pontuation",
			"scope": [
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
        "keyword.operator.comparison.js.jsx",
        "keyword.operator.logical.js.jsx",
        "meta.block.js.jsx",
        "meta.arrow.js.jsx",
        "meta.block.js.jsx",
        "meta.arrow.js.jsx",
			],
			 "settings": {
				"foreground": "#5c93b8",
			 }
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter",
        "entity.name.tag.ts.tsx",
        "entity.name.tag.ts",
        "entity.name.tag.js",
        "entity.name.tag.js.jsx",
			],
			"settings": {
				"foreground": "#bf9346"
			}
		},
    {
      "name": "Tag jsx tsx",
      "scope": [
        "support.class.component.js.jsx",
        "support.class.component.tsx"
      ],
      "settings": {
				"foreground": "#bf9346",
        "fontStyle": "italic"
			}
    },
    {
      "name": "Tag children",
      "scope": [
        "meta.jsx.children.js.jsx"
      ],
       "settings": {
        "foreground": "#c7cbd6",
       }
    },
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#be6d8d",
        "fontStyle": "bold"
      }
    },
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#c7cbd6",
				"fontStyle": "bold"
			}
		},
    {
      "name": "String",
      "scope": [
        "string",
        "meta.embedded.line",
        "string.quoted.double",
        "string.quoted.single",
        "string.template",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#be6d8d"
      }
    },
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"constant.other.symbol",
				"constant.other.key",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#4391d4"
			}
		},
		{
			"name": "Class declaration, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#c7cbd6",
        "fontStyle": "bold"
			}
		},

		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
        "entity.name.function.ts",
        "entity.name.command.shell"
			],
			"settings": {
				"foreground": "#b48a49",
        "fontStyle": ""
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type",
        "storage.type.primitive.java",
        "storage.type.java",
        "storage.type.js.jsx"
			],
			"settings": {
				"foreground": "#5c93b8",
        "fontStyle": "italic"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "#b48a49",
        "fontStyle": "italic"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#97564b"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b48a49"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#b48a49"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#97564b"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#f33c86"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#97564b"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "JSON Key",
			"scope": [
				"source.json meta.structure.dictionary.json support.type.property-name.json"
			],
			"settings": {
				"foreground": "#c7cbd6",
			}
		},
		{
			"name": "Markdown - Plain",
			"scope": [
				"text.html.markdown",
				"punctuation.definition.list_item.markdown"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "Markdown - Markup Raw Inline",
			"scope": [
				"text.html.markdown markup.inline.raw.markdown"
			],
			"settings": {
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markdown - Markup Raw Inline Punctuation",
			"scope": [
				"text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"
			],
			"settings": {
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markdown - Heading",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#b48a49"
			}
		},
		{
			"name": "Markup - Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markup - Bold",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markup - Bold-Italic",
			"scope": [
				"markup.bold markup.italic",
				"markup.italic markup.bold",
				"markup.quote markup.bold",
				"markup.bold markup.italic string",
				"markup.italic markup.bold string",
				"markup.quote markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markup - Underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markdown - Blockquote",
			"scope": [
				"markup.quote punctuation.definition.blockquote.markdown"
			],
			"settings": {
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markup - Quote",
			"scope": [
				"markup.quote"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown - Link",
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "Markdown - Link Description",
			"scope": [
				"string.other.link.description.title.markdown"
			],
			"settings": {
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markdown - Link Anchor",
			"scope": [
				"constant.other.reference.link.markdown"
			],
			"settings": {
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markup - Raw Block",
			"scope": [
				"markup.raw.block"
			],
			"settings": {
				"foreground": "#be6d8d"
			}
		},
		{
			"name": "Markdown - Raw Block Fenced",
			"scope": [
				"markup.raw.block.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000050"
			}
		},
		{
			"name": "Markdown - Fenced Bode Block",
			"scope": [
				"punctuation.definition.fenced.markdown"
			],
			"settings": {
				"foreground": "#00000050"
			}
		},
		{
			"name": "Markdown - Fenced Bode Block Variable",
			"scope": [
				"markup.raw.block.fenced.markdown",
				"variable.language.fenced.markdown",
				"punctuation.section.class.end"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "Markdown - Fenced Language",
			"scope": [
				"variable.language.fenced.markdown"
			],
			"settings": {
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markdown - Separator",
			"scope": [
				"meta.separator"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#65737E"
			}
		},
		{
			"name": "Markup - Table",
			"scope": [
				"markup.table"
			],
			"settings": {
				"foreground": "#c7cbd6"
			}
		},
		{
			"name": "Markdown - Headings",
			"scope": [
				"entity.name.section.markdown",
			],
			"settings": {
				"foreground": "#4391d4",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.inline.raw.string.markdown",
			"settings": {
				"foreground": "#b48a49",
				"fontStyle": "bold"
			}
		},
    {
      "name": "Package strings names",
      "scope": [
        "entity.name.type.package",
        "entity.name.type.package.go",
        "storage.modifier.package",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#b48a49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import strings",
      "scope": ["entity.name.import.go", "storage.modifier.import.java"],
      "settings": {
        "foreground": "#c7cbd6",
        "fontStyle": "italic bold"
      }
    }
	]
}
